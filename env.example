# AWS credentials for S3 storage
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-east-1

# S3 bucket configuration
S3_BUCKET_NAME=llm-finetuning-data

# Optional: MLflow configuration (for future use)
MLFLOW_TRACKING_URI=https://your-mlflow-server/
MLFLOW_EXPERIMENT_NAME=llm-finetuning

# Optional: Model configuration
MODEL_HUB_TOKEN=your_huggingface_token

# Optional: Compute resources (for future use)
MAX_MEMORY=32GB
NUM_WORKERS=4
